	<script>
		function download_config(host,port,user,pass,pub_topic,sub_topic)
		{
			config={}
			
			config['host']=host?host:"";
			config['port']=port?port:"";
			
			config['user']=user?user:"";
			config['pass']=pass?pass:"";
			
			config['pub_topic']=pub_topic?pub_topic:"";
			config['sub_topic']=sub_topic?sub_topic:"";

			console.log(jsyaml.safeDump(config));

		}
		
		download_config('localhost','1883','sam','pass','xyz','abc')
	</script>
